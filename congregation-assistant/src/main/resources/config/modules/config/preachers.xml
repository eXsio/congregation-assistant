<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
        http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.2.xsd">

    <!-- override locale provider to hadle polish unless forced -->
    <bean id="caPreachersConfigModule" class="pl.exsio.ca.module.config.preachers.CaPreachersConfigModule" scope="ui">
        <property name="caEntities" ref="caEntityFactory" />
        <property name="securityEntities" ref="securityEntityFactory" />
        <property name="preacherEntityProvider" ref="preacherEntityProvider" />
        <property name="priviledgeEntityProvider" ref="preacherPriviledgeEntityProvider" />
        <property name="preacherAssignmentEntityProvider" ref="preacherAssignmentEntityProvider" />
        <property name="serviceGroupEntityProvider" ref="serviceGroupEntityProvider" />
        <property name="caRepositories" ref="caRepositoryProvider" />
    </bean>
    
    
  
</beans>